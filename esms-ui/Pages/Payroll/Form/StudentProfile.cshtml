            <div class="card-body">
                <table id="data_table" class="table table-responsiv table-striped table-bordered" style="width:100% ;" >
                    <thead>
                        <tr>
                            <th style="width: 10%;"></th>
                            <th>NO</th>
                            <th>SSB</th>
                            <th>NAME</th>
                            <th>IDENTITY</th>
                            <th>PHONE</th>
                            <th>DATEOFBIRTH</th>
                            <th>EMAIL</th>
                            <th>ACTIVE</th>
                        </tr>
                    </thead>
                    <tbody>
                </table>
                <div class="text-center" id="table_loading_image" ><img src="~/img/brands/pro_gif.gif" alt=""></div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="data_Model" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
           <div class="modal-header p-3" style="background-color: #3e4676;">
                <h4 class="modal-title text-white"> @ViewData["Title"]</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" class="text-white">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="create_form">
                    <fieldset id="SubmitForm">
                        <div class="row">
                            <div class="col-lg-7">
                                <div class="col-lg-12">
                                   <div class="image-container">
                                        <img  class="rounded border mr-2 mb-2" id="profile-image" src="~/img/images/profile.jpg" alt="Placeholder" width="140" height="140">
                                        <input type="file" id="image-upload-input" accept="image/*" style="display: none;">
                                        <div class="cancel-button" id="set_default_image">
                                            <i class="fas fa-trash"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4 did-floating-label-content" >
                                <input class="did-floating-input" id="txt_code" type="text">
                                <label class="did-floating-label" >Code</label>
                            </div>
                            <div class="col-lg-1 did-floating-label-content" >
                                <button type="button" id="btn_codegenerate" class="btn btn-primary align-middle   m-1" style="background-color: #2c497d;color:white"><i class="align-middle fas fa-fw fa-sync-alt"></i></button>
                            </div>
                        </div> 
                        <hr>
                        <div class="tab">
								<ul class="nav nav-tabs" role="tablist">
									<li class="nav-item"><a class="nav-link active" href="#tab-1" data-toggle="tab" role="tab"><h5 class="pt-2 pl-2 " >PERSONAL</h5></a></li>
                                    <li class="nav-item"><a class="nav-link" href="#tab-2" data-toggle="tab" role="tab"><h5 class="pt-2 pl-2 " >COURSES</h5></a></li>
									<li class="nav-item"><a class="nav-link" href="#tab-3" data-toggle="tab" role="tab"><h5 class="pt-2 pl-2 " >ATTACHMENT</h5></a></li>
								</ul>
								<div class="tab-content">
									<div class="tab-pane active" id="tab-1" role="tabpanel">
                                        <div class="row form-group m-2">
                                            <div class="col-lg-4 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_name" type="text">
                                                <label class="did-floating-label">Name</label>
                                            </div>
                                            <div class="col-lg-4 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_lastname" type="text">
                                                <label class="did-floating-label">Last Name</label>
                                            </div>
                                             <div class="col-lg-4 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_fathername" type="text">
                                                <label class="did-floating-label">Father Name</label>
                                            </div>
                                            <div class="col-lg-12 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_address" type="text">
                                                <label class="did-floating-label">Postal Address</label>
                                            </div>
                                            <div class="col-lg-4 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_email" type="text">
                                                <span>eg : abc@gmail.com</span>
                                                <label class="did-floating-label">Email</label>
                                            </div>
                                            <div class="col-lg-4 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_contact" type="number">
                                                <label class="did-floating-label">Tel/ Cell No</label>
                                            </div>
                                            <div class="col-lg-4 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_cnic" type="text">
                                                 <span>eg : xxxxx-xxxxxxx-x</span>
                                                <label class="did-floating-label">NIC #</label>
                                            </div>
                                            <div class="col-lg-4 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_dob" type="date">
                                                <label class="did-floating-label">Birth Date</label>
                                            </div>
                                            <div class="col-lg-8 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_dobplace" type="text">
                                                <label class="did-floating-label">Birth Place</label>
                                            </div>
                                            <div class="col-lg-4 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_rank" type="text">
                                                <label class="did-floating-label">Rank</label>
                                            </div>
                                            <div class="col-lg-4 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_nationallity" type="text">
                                                <label class="did-floating-label">Nationality</label>
                                            </div>
                                            <div class="col-lg-4 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_ssb" type="text">
                                                <label class="did-floating-label">CDC/SSB #</label>
                                            </div>
                                            <div class="col-lg-4 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_cocheld" type="text">
                                                <label class="did-floating-label">COC Held</label>
                                            </div>
                                            <div class="col-lg-4 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_coc" type="text">
                                                <label class="did-floating-label">COC #</label>
                                            </div>
                                            <div class="col-lg-4 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_passport" type="text">
                                                <label class="did-floating-label">Passport #</label>
                                            </div>
                                           <div class="col-lg-8" >
                                                <label class="col-form-label col-lg-6" id="txt_id" ></label>
                                                    <label class="col-form-label col-lg-6" id="txt_no" ></label>
                                            </div>
                                            <div class="col-lg-2" style="text-align:right;">
                                                <label class="custom-control custom-checkbox m-0">
                                                    <input type="checkbox" id="ck_fres" class="custom-control-input">
                                                    <span class="custom-control-label">FRESHER</span>
                                                </label>
                                            </div>
                                            <div class="col-lg-2" style="text-align:right;">
                                                <label class="custom-control custom-checkbox m-0">
                                                    <input type="checkbox" id="ck_act" class="custom-control-input">
                                                    <span class="custom-control-label">ACTIVE</span>
                                                </label>
                                            </div>
                                            
                                        </div>
									</div>
                                    
                                    <div class="tab-pane" id="tab-2" role="tabpanel">
                                        <div class="row form-group m-2">
                                            
                                            <div class="col-lg-6 did-floating-label-content">
                                                <select class="did-floating-select" id="sel_coursecat" >
                                                    
                                                </select>
                                                <label class="did-floating-label">Course Category</label>
                                            </div>
                                             <div class="col-lg-6 did-floating-label-content">
                                                <select class="did-floating-select" id="sel_course" >
                                                    
                                                </select>
                                                <label class="did-floating-label">Course</label>
                                            </div>
                                            <div class="col-lg-3 did-floating-label-content" hidden >
                                                <input class="did-floating-input" readonly id="txt_fees" type="text">
                                                <label class="did-floating-label">Fee</label>
                                            </div>
                                            <div class="col-lg-3 did-floating-label-content" hidden>
                                                <input class="did-floating-input" id="txt_dis" type="number">
                                                <label class="did-floating-label">Discount %</label>
                                            </div>
                                            <div class="col-lg-3 did-floating-label-content" hidden >
                                                <input class="did-floating-input" id="txt_disamt" type="number">
                                                <label class="did-floating-label">Discount AMT</label>
                                            </div>
                                            <div class="col-lg-3 did-floating-label-content" hidden>
                                                <input class="did-floating-input" id="txt_amt" readonly type="number">
                                                <label class="did-floating-label">Net Amount</label>
                                            </div>
                                            <div class="col-lg-5 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_startdate" type="date">
                                                <label class="did-floating-label">Start</label>
                                            </div>
                                             <div class="col-lg-5 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_enddate" type="date">
                                                <label class="did-floating-label">End</label>
                                            </div>
                                             <div class="col-lg-2 did-floating-label-content" >
                                                <button type="button" class="btn btn-secondary ml-2 pt-2 pr-4 pl-4 pb-2" id="btn_add_course"
                                                    style="background-color: #2c497d;color:white" >Add</button>
                                            </div>
                                            <div class="card-body table-responsive">
                                                <table id="student_course_table" class="table table-hover" style="width:100% ;" >
                                                    <thead>
                                                        <tr>
                                                            <th></th>
                                                            <th>S#</th>
                                                            <th>COURSE CATGEORY</th>
                                                            <th>COURSE</th>
                                                            <th>START</th>
                                                            <th>END</th>
                                                            <th>FEES</th>
                                                            <th>DIS %</th>
                                                            <th>DIS AMT</th>
                                                            <th>NET AMT</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                </table>
                                            </div>
                                        </div>
									</div>
                                    <div class="tab-pane" id="tab-3" role="tabpanel">
                                        <div class="row form-group m-2">
                                            <div class="col-lg-7 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_file" type="file">
                                                <label class="did-floating-label">Image</label>
                                            </div>
                                            <div class="col-lg-5 did-floating-label-content">
                                                <select class="did-floating-select" id="sel_doctype" >
                                                    
                                                </select>
                                                <label class="did-floating-label">Document Type</label>
                                            </div>
                                                <div class="col-lg-10 did-floating-label-content" >
                                                <input class="did-floating-input" id="txt_rem" type="text">
                                                <label class="did-floating-label">Remarks</label>
                                            </div>
                                                <div class="col-lg-2 did-floating-label-content" >
                                                <button type="button" class="btn btn-secondary ml-2 pt-2 pr-4 pl-4 pb-2" id="btn_add_attch"
                                                    style="background-color: #2c497d;color:white" >Add</button>
                                            </div>
                                            <div class="card-body table-responsive">
                                                <table id="student_attach_table" class="table table-border table-hover" style="width:100% ;" >
                                                    <thead>
                                                        <tr>
                                                            <th></th>
                                                            <th>S#</th>
                                                            <th>DOCUMENT</th>
                                                            <th>IMAGE</th>
                                                            <th>REMARKS</th>
                                                            <th>FILE</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
								</div>
							</div>
                            <div class="row m-2">
                                <div class="col-lg-12" style="text-align:right;">
                                    <button type="button" class="btn btn-secondary" data-dismiss="modal"
                                    style="background-color: #2c497d;color:white" >Close</button>
                                    <input id="btn_sav" type="button" value="Save"
                                    class="btn " style="background-color: #7566cc;color:white" />
                                    <input id="btn_upd" type="button" value="Update" 
                                    class="btn"  style="background-color: #7566cc;color:white"  />
                                </div>
                            </div>
                    </fieldset>
                </form>
            </div>
        </div>
    </div>
</div>
<!-- END  modal -->

<!-- Student Upload modal -->
<div class="modal fade" id="upload_student_file_model" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
           <div class="modal-header p-3" style="background-color: #3e4676;">
                <h4 class="modal-title text-white"> @ViewData["Title"] UPLOAD FILE
                   
                </h4>
                <center><span id="loader_export_save"><img src="~/img/brands/process_gif.gif" alt=""  width="20%" ></span></center>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" class="text-white">&times;</span>
                </button>
            </div>
            <div class="modal-body ">
                <form id="create_form">
                    <fieldset >
                            <div class="row ">
                                <div class="col-lg-6 did-floating-label-content">
                                    <select class="did-floating-select" id="sel_exp_coursecat" >
                                        
                                    </select>
                                    <label class="did-floating-label">Course Category</label>
                                </div>
                                    <div class="col-lg-6 did-floating-label-content">
                                    <select class="did-floating-select" id="sel_exp_course" >
                                        
                                    </select>
                                    <label class="did-floating-label">Cource</label>
                                </div>
                                <div class="col-lg-12 did-floating-label-content" >
                                    <input class="did-floating-input" type="file" id="access-file-input">
                                    <label class="did-floating-label" >File</label>
                                </div>
                            </div> 
                             <div class="row m-2">
                                <div class="col-lg-12" style="text-align:right;">
                                    <button type="button" class="btn btn-secondary" data-dismiss="modal"
                                    style="background-color: #2c497d;color:white" >Close</button>
                                    <input id="btn_exp_sav" type="button" value="Save"
                                    class="btn " style="background-color: #7566cc;color:white" />
                                    @* <input id="btn_upd" type="button" value="Update" 
                                    class="btn"  style="background-color: #7566cc;color:white"  /> *@
                                </div>
                            </div>
                             <hr>
                            <div class="table-responsive">
                                <table id="student_data_table" class="table table-striped table-bordered">  
                                </table>
                            </div>
                    </fieldset>
                </form>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="student_info_Model" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg " role="document">
        <div class="modal-content">
            @* <div class="modal-header p-3" style="background-color: #3e4676;">
                <h4 class="modal-title text-white"> STUDENT INFORMATION</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" class="text-white">&times;</span>
                </button>
                
            </div> *@
            <div class="modal-body ">
                <form id="create_form">
                    <fieldset id="SubmitForm">
                        <div class="row ">
                            <div class="col-lg-12 text-center" >
                                <img  class="rounded border mb-2" id="" src="~/img/brands/einstituteLogo.png" width="30%" >
                            </div>
                        </div>
                        <hr>
                        <div class="row ">
                            <div class="col-lg-3" >
                                <div class="col-lg-12 " >
                                    <div class="image-container" >
                                        <img  class="rounded border mb-2" id="info_profile-image" src="~/img/images/profile.jpg" alt="Placeholder" width="200" height="200">
                                    </div>
                                </div>
                            </div>
