@page
@{
    ViewData["Title"] = "STUDENT CHECK LIST";
    Layout = "_Layout";
}
<div class="container-fluid">
    <div class="row">
        <div class="col-12 " >
            <h4 class="header-title" >
                @ViewData["Title"]
            </h4>
        </div>
    </div>
</div>

